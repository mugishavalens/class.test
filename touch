#include <stdio.h>

int main() {
    int number, result;
    
    // Ask the user for input
    printf("Enter a number: ");
    scanf("%d", &number);
    
    // Multiply the entered number by 2
    result = number * 2;
    
    // Display the result
    printf("Result after multiplying by 2: %d\n", result);
    
    return 0;
}

